{
	"slug": "email-profile-finder",
	"description": "Find LinkedIn Profiles from a list of emails",
	"image": "https://s3-eu-west-1.amazonaws.com/phantombuster-static/api-store/Email-Extractor/Email+Extractor+-+Logo.png",
	"hidden": true,
	"new": true,
	"gradientBegin": "#4CA1AF",
	"gradientEnd": "#28485B",
	"argumentHelpAnchor": "#section_argument",
	"defaultArgument": {
		"sessionCookie": "your_session_cookie",
		"spreadsheetUrl": "https://docs.google.com/spreadsheets/d/1qWQ5t-pFaibQQoltH05uH0uw_t_fArpZoYTAYh4k1Gg"
	},
	"outputDescription": {
		"profileUrl": "LinkedIn profile URL"
	},
	"defaultArgumentAllowed": false,
	"argumentForm": {
		"enabled": true,
		"alpaca": {
			"schema": {
				"properties": {
					"spreadsheetUrl": {
						"required": true
					}
				}
			},
			"options": {
				"fields": {
					"sessionCookie": {
						"label": "Session cookie <a href=\"/api-store/9058/email-profile-finder#section_argument\" target=\"_blank\">(?)</a>",
						"placeholder": "Paste your session cookie here"
					},
					"spreadsheetUrl": {
						"type": "url",
						"helpers": [
							"Enter either a Google Spreadsheet URL or a CSV URL (make sure your file is publicly available)",
							"<b>Please be gentle.</b> This API will manipulate LinkedIn on your behalf. Scraping 100+ profiles in a short period of time is not recommended."
						],
						"label": "Spreadsheet URL of emails to process <a href=\"/api-store/9058/email-profile-finder#section_5addagooglespreadsheet\" target=\"_blank\">(?)</a>"
					},
					"columnName": {
						"label": "Column name from which to get profile URLs",
						"placeholder": "Emails",
						"helper": "By default, we'll use the first column of your spreadsheet (column A)"
					},
					"numberOfLinesPerLaunch": {
						"label": "Number of lines to process per launch (all of them if empty) <a href=\"/api-store/9058/email-profile-finder#section_repetition_setup\" target=\"_blank\">(?)</a>",
						"placeholder": 10,
						"inputType": "number"
					}
				}
			}
		}
	},
	"argumentSchema": {
		"type": "object",
		"properties": {
			"sessionCookie": {
				"type": "string",
				"required": true,
				"minLength": 15
			},
			"spreadsheetUrl": {
				"type": "string"
			},
			"columnName": {
				"type": "string"
			},
			"numberOfLinesPerLaunch": {
				"type": "number"
			}
		}
	}
}
