			},
			"sessionCookieCUser": {
				"type": "string",
				"required": true,
				"minLength": 6
			},
			"sessionCookieXs": {
				"type": "string",
				"required": true,
				"minLength": 15
			},
			"sessionCookieAuthToken": {
				"type": "string",
				"required": false,
				"minLength": 15
			},
			"spreadsheetUrl": {
				"type": "string",
				"required": true
			},
			"columnName": {
				"type": "string"
			},
			"numberOfLinesPerLaunch": {
				"type": "number",
				"minimum": 1
			},
			"csvName": {
				"type": "string",
				"required": false
			}
		}
