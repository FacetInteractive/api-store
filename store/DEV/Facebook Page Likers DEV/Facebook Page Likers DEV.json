{
    "slug": "facebook-page-likers",
    "description": "Extract a list of all the profiles that liked any Facebook page",
    "image": "https://phantombuster.imgix.net/api-store/Instagram_Profile_Scrapper/inst_prfl_scrppr.png",
    "hidden": true,
    "new": true,
    "gradientBegin": "#373B44",
    "gradientEnd": "#4286f4",
    "argumentHelpAnchor": "#TODO",
	"defaultArgument": {
        "sessionCookieCUser": "your_c-user_session_cookie",
        "sessionCookieXs": "your_xs_session_cookie"
	},
	"outputDescription": {
		"profileUrl": "Facebook profile URL",
		"name": "Full name",
		"profilePictureUrl ": "Profile picture URL"
	},
	"defaultArgumentAllowed": false,
	"argumentForm": {
		"enabled": true,
		"alpaca": {
			"options": {
				"fields": {
                    "sessionCookieCUser": {
                        "label": "Session cookie c_user <a href=\"/api-store/8368/facebook-post-likers#section_argument\" target=\"_blank\">(?)</a>",
                        "placeholder": "Paste your c_user session cookie here"
                    },
                    "sessionCookieXs": {
                        "label": "Session cookie xs <a href=\"/api-store/8368/facebook-post-likers#section_argument\" target=\"_blank\">(?)</a>",
                        "placeholder": "Paste your xs session cookie here"
                    },
					"spreadsheetUrl": {
						"type": "url",
						"helpers": [
							"Enter either a Google Spreadsheet URL, a CSV URL (make sure your file is publicly available), or directly an Facebook page URL"
						],
						"label": "Facebook page or Spreadsheet URL <a href=\"/api-store/8368/facebook-post-likers#section_5addagooglespreadsheet\" target=\"_blank\">(?)</a>"
					},
					"columnName": {
						"label": "Column name from which to get profile URLs",
						"placeholder": "Facebook Page link",
						"helper": "By default, we'll use the first column of your spreadsheet (column A)"
					},
					"csvName": {
                        "label": "Name of resulting CSV file",
                        "placeholder": "result"
					},
				    "maxLikers": {
						"label": "Maximum number of likers to extract",
						"helper": "By default, there is no limit"
					}
				}
			}
		}
	},
	"argumentSchema": {
		"type": "object",
		"properties": {
            "sessionCookieCUser": {
                "type": "string",
                "required": true,
                "minLength": 8
            },
            "sessionCookieXs": {
                "type": "string",
                "required": true,
                "minLength": 15
            },
			"spreadsheetUrl": {
				"type": "string",
				"required": true
			},
			"columnName": {
				"type": "string"
			},
            "csvName": {
				"type": "string",
				"required": false
	   	 	},
			"maxLikers": {
				"type": "number",
				"required": false
			}
		}
	}
}
